package day0108;

public class For_09 {

	public static void main(String[] args) {
		
		System.out.println("\n\n** for문 가로로 1~10출력 **");
		int i;
		for(i=1;i<=10;i++) {
			System.out.print(i+"  ");
		}
		
		
		
		
		
		
		
		
		System.out.println("\n\n** for문 가로로 10~1출력 **");
		
		
		
		
		
		
		
		
		
		
		System.out.println("\n\n** for문 배수, continue **");
		
		
		
		
		
		
		
		
		
		
		
	}

}
