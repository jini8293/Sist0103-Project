package day0115;

import java.util.HashMap;
import java.util.Map;

//key와 value로 이루어지는 데이터의 집합
//key로 구분
//순서가 유지되지 않는다
//키는 중복 허용하지 않고 값은 중복을 허용한다
public class MapEx_11 {

	public static void main(String[] args) {

		Map<String, String> map = new HashMap<String, String>();
		map.put("name", "강호동");
		map.put("age", "33");

	}

}
