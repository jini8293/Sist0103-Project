package day0108;

public class For_10 {

	public static void main(String[] args) {
		
		System.out.println("** for문 가로로 10~1출력 **\n");
		int i;
		for(i=1;i<=10;i++) {
			System.out.print(i+"  ");
		}

		System.out.print("\n** for문 빠져나온 i의 값: "+i+"\n\n");
	
		int a;
		for(a=0;a<=50;a+=5) {
			System.out.print(a+"  ");
		}

		System.out.println("\n** for문 빠져나온 a의 값: "+a);
		
	}

}
