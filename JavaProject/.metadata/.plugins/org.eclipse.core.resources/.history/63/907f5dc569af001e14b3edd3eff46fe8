package day0110;

public class CardTest {

	public static void main(String[] args) {
		
		Card.width=10;
		Card.height=20;
		
		//클래스가 로딩될때 변수명도 바로 로딩
		System.out.println(Card.width);
		System.out.println(Card.height);
		
		//인스턴스 벼수 생성
		Card c1 =new Card();
		System.out.println(c1.kind);
		System.out.println(c1.number);
		
		c1.kind="Heart";
		c1.number=7;
		
		Card c2 =new Card();
		c2.kind="Spade";
		c2.number=6;
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
