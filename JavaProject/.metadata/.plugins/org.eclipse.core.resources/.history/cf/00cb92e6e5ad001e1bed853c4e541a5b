package day0108;

public class QuizFor_12 {

	public static void main(String[] args) {
		
	
		System.out.println("1. 1 2 4 5 7 8 10 (continue활용)");
		int k;
		for(k=1;k<=10;k++) {
			if(k%3==0) {
				continue;
			}
			System.out.print(k+"  ");
		}
		
		System.out.println("\n2. 홀수값:1 홀수값:3 홀수값:5 홀수값:7 홀수값:9");
		int a;
		for (a=1;a<=10; a++) {
			if(a%2==0) {
				continue;
			}
			System.out.print("홀수값: "+a+"  ");
		}
		

	}

}
